{
  "name": "@mcpn/core",
  "version": "0.0.1",
  "description": "MCP workflow orchestration and registry tool with presets for thinking, coding and more",
  "type": "module",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "workflows",
    "prompts",
    "prompt engineering",
    "ai",
    "mcpn",
    "mcp network"
  ],
  "contributors": [
    {
      "name": "Ted Werbel",
      "email": "ted@agentdesk.ai"
    },
    {
      "name": "Kevin Kern",
      "email": "kevin@instructa.org"
    }
  ],
  "bugs": "https://github.com/dx-zero/mcpn/issues",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "tsx src/server.ts",
    "dev:verbose": "NODE_ENV=development tsx src/server.ts",
    "start": "node dist/server.js",
    "inspect": "tsc && mkdir -p dist/presets && cp src/presets/*.yaml dist/presets/ && npx @modelcontextprotocol/inspector node -- dist/server.js",
    "build": "unbuild",
    "prepublishOnly": "npm run build",
    "update": "npm version patch && npm publish",
    "test": "tsc && mkdir -p dist/presets && cp src/presets/*.yaml dist/presets/ && node --loader ts-node/esm --experimental-specifier-resolution=node node_modules/mocha/bin/mocha.js ./__tests__/**/*.test.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "mcpn": "workspace:*",
    "js-yaml": "^4.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.0.0",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.6.0",
    "typescript": "^5.7.3",
    "@modelcontextprotocol/sdk": "catalog:prod",
    "ansis": "catalog:prod",
    "debug": "catalog:prod",
    "pathe": "catalog:prod",
    "zod": "catalog:prod"
  },
  "packageManager": "pnpm@10.6.3"
}
