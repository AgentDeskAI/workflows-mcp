{
  "name": "mcpn",
  "version": "0.1.0",
  "description": "MCP workflow orchestration and registry tool with presets for thinking, coding and more",
  "type": "module",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "workflows",
    "prompts",
    "prompt engineering",
    "ai",
    "mcpn",
    "mcp network"
  ],
  "contributors": [
    {
      "name": "Ted Werbel",
      "email": "ted@agentdesk.ai"
    },
    {
      "name": "Kevin Kern",
      "email": "kevin@instructa.org"
    }
  ],
  "bugs": "https://github.com/dx-zero/mcpn/issues",
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "bin": {
    "mcpn": "./dist/cli.mjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist",
    "index.js"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "npm run build",
    "release": "npm publish",
    "release:alpha": "npm publish --tag alpha"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "@types/semver": "^7.7.0",
    "c12": "^3.0.2",
    "chokidar": "^4.0.3",
    "citty": "^0.1.6",
    "clipboardy": "^4.0.0",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "fuse.js": "^7.1.0",
    "giget": "^2.0.0",
    "h3": "^1.15.1",
    "httpxy": "^0.1.7",
    "jiti": "^2.4.2",
    "listhen": "^1.9.0",
    "magicast": "^0.3.5",
    "nitropack": "npm:nitropack-nightly",
    "nypm": "^0.6.0",
    "ofetch": "^1.4.1",
    "ohash": "^2.0.11",
    "pathe": "^2.0.3",
    "perfect-debounce": "^1.0.0",
    "pkg-types": "^2.1.0",
    "rollup": "^4.38.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "scule": "^1.3.0",
    "semver": "^7.7.1",
    "std-env": "^3.8.1",
    "tinyexec": "^1.0.1",
    "typescript": "^5.8.2",
    "ufo": "^1.5.4",
    "unbuild": "^3.5.0",
    "unplugin-purge-polyfills": "^0.0.7",
    "vitest": "^3.1.1",
    "youch": "^4.1.0-beta.6"
  },
  "packageManager": "pnpm@10.6.3"
}
