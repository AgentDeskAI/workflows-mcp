    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "fuse.js": "^7.1.0",
    "giget": "^2.0.0",
    "httpxy": "^0.1.7",
    "jiti": "^2.4.2",
    "listhen": "^1.9.0",
    "magicast": "^0.3.5",
    "mri": "^1.2.0",
    "nitropack": "npm:nitropack-nightly",
    "nypm": "^0.6.0",
    "ofetch": "^1.4.1",
    "ohash": "^2.0.11",
