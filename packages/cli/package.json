    "ufo": "^1.5.4",
    "unplugin-purge-polyfills": "^0.0.7",
    "youch": "^4.1.0-beta.6",
    "@types/chai": "^4.3.5",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.0.0",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.6.0",
    "typescript": "^5.7.3",
